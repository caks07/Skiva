<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context=".UI.detail_dokter">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="28dp"
        android:text="Profile Dokter"
        android:textColor="#17C2EC"
        android:textSize="20sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/button_back"
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="16dp"
        android:background="@android:color/transparent"
        android:src="@drawable/b_back"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <FrameLayout
        android:layout_width="410dp"
        android:layout_height="611dp"
        android:background="@drawable/rounded_corners"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/textView"
        app:layout_constraintVertical_bias="0.292">

        <FrameLayout
            android:layout_width="331dp"
            android:layout_height="272dp"
            android:layout_marginLeft="130px"
            android:layout_marginTop="800px"
            android:background="#FFFFFF"

            >

            <Button
                android:id="@+id/button"
                android:layout_width="133dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="70px"
                android:layout_marginTop="550px"
                android:background="@drawable/button_background"
                android:text="Chat"
                android:textColorLink="#00CEFF" />

            <Button
                android:id="@+id/button3"
                android:layout_width="133dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="450px"
                android:layout_marginTop="550px"
                android:background="@drawable/button_background"
                android:text="Call"
                android:textColorLink="#00CFFF" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_marginLeft="370px"
                android:layout_marginTop="110px"
                tools:srcCompat="@drawable/doker2" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="220px"
                android:layout_marginTop="300px"
                android:text="Contact"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/contact"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="400px"
                android:layout_marginTop="300px"
                android:background="#00281C1C"
                android:text="0884 6474 7464"
                android:textColor="#000000"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/spesialisasi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="400px"
                android:layout_marginTop="350px"
                android:background="#00281C1C"
                android:text="Penyakit Kulit"
                android:textColor="#000000"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/experience"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="400px"
                android:layout_marginTop="400px"
                android:background="#00281C1C"
                android:text="10 tahun"
                android:textColor="#000000"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="220px"
                android:layout_marginTop="350px"
                android:text="Spesialis"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="220px"
                android:layout_marginTop="400px"
                android:text="Experience"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/deskrip"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="220px"
                android:layout_marginTop="450px"
                android:text="Deskripsi"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/deskripsi"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="400px"
                android:layout_marginTop="450px"
                android:background="#00281C1C"
                android:text="deskripsi"
                android:textColor="#000000"
                android:textSize="12sp" />

        </FrameLayout>

        <ImageButton
            android:id="@+id/mapView"
            android:layout_width="320dp"
            android:layout_height="180dp"
            android:layout_marginLeft="110px"
            android:layout_marginTop="150px"
            android:background="@drawable/img_rumah_sakit" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="110px"
            android:layout_marginTop="70px"
            android:text="RS. ICIKIWIR"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/nama"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="400px"
            android:layout_marginTop="850px"
            android:text="Dr. AgraCawiksana"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="110px"
            android:layout_marginTop="640px"
            android:text="Rumah Sakit Icikiwir Lurr"
            android:textColor="#000000"
            android:textSize="12sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="110px"
            android:layout_marginTop="690px"
            android:text="Jalan Ngawi, bola bali nggo dolanan"
            android:textColor="#76000000"
            android:textSize="12sp"
            android:textStyle="bold" />
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>