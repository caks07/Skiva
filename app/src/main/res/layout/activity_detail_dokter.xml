<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#0B153C"
    android:backgroundTint="#0B153C"
    tools:context=".UI.detail_dokter">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#00FFFFFF"
        android:backgroundTint="#17C2EC"
        android:drawableTint="#000000"
        android:shadowColor="#000000"
        android:text="Profile Dokter"
        android:textColor="#17C2EC"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.05" />

    <ImageButton
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:background="@android:color/transparent"
        android:src="@drawable/b_back"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="9dp" />

    <ImageButton
        android:layout_width="34dp"
        android:layout_height="34dp"
        android:background="@android:color/transparent"
        android:src="@drawable/b_notif"
        tools:layout_editor_absoluteX="345dp"
        tools:layout_editor_absoluteY="9dp" />

    <FrameLayout
        android:layout_width="432dp"
        android:layout_height="644dp"
        android:background="@drawable/rounded_corners"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_editor_absoluteX="-11dp">

        <FrameLayout
            android:layout_width="331dp"
            android:layout_height="272dp"
            android:layout_marginLeft="130px"
            android:layout_marginTop="800px"
            android:background="@drawable/combined_background"

            >

            <Button
                android:id="@+id/button"
                android:layout_width="133dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="70px"
                android:layout_marginTop="550px"
                android:background="@drawable/button_background"
                android:text="Chat"
                android:textColorLink="#00CEFF" />

            <Button
                android:id="@+id/button3"
                android:layout_width="133dp"
                android:layout_height="wrap_content"
                android:layout_marginLeft="450px"
                android:layout_marginTop="550px"
                android:background="@drawable/button_background"
                android:text="Call"
                android:textColorLink="#00CFFF" />

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginLeft="370px"
                android:layout_marginTop="150px"
                tools:srcCompat="@tools:sample/avatars" />

            <TextView
                android:id="@+id/textView6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="220px"
                android:layout_marginTop="300px"
                android:text="Nama:"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="370px"
                android:layout_marginTop="300px"
                android:background="#00281C1C"
                android:text="Bayoe wicaksana pradipta"
                android:textColor="#000000"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="370px"
                android:layout_marginTop="350px"
                android:background="#00281C1C"
                android:text="69420"
                android:textColor="#000000"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="370px"
                android:layout_marginTop="400px"
                android:background="#00281C1C"
                android:text="081 123 456 7890"
                android:textColor="#000000"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="220px"
                android:layout_marginTop="350px"
                android:text="No antri:"
                android:textSize="12sp" />

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="220px"
                android:layout_marginTop="400px"
                android:text="Contact:"
                android:textSize="12sp" />

        </FrameLayout>

        <com.google.android.gms.maps.MapView
            android:id="@+id/mapView"
            android:layout_width="244dp"
            android:layout_height="121dp"
            android:layout_marginLeft="250px"
            android:layout_marginTop="150px"
            android:background="@drawable/rounded_corners" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="250px"
            android:layout_marginTop="70px"
            android:text="RS. ICIKIWIR"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="400px"
            android:layout_marginTop="850px"
            android:text="Dr. AgraCawiksana"
            android:textColor="#000000"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="230px"
            android:layout_marginTop="490px"
            android:text="Rumah Sakit Icikiwir Lurr"
            android:textColor="#000000"
            android:textSize="12sp"
            android:textStyle="bold" />

        <TextView
            android:id="@+id/textView4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="230px"
            android:layout_marginTop="530px"
            android:text="Jalan Ngawi, bola bali nggo dolanan"
            android:textColor="#76000000"
            android:textSize="12sp"
            android:textStyle="bold" />
    </FrameLayout>

</androidx.constraintlayout.widget.ConstraintLayout>